<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.jumpcraft08.musiclibrary.controller.TableController">
    <children>
        <TableView fx:id="TableSong" prefHeight="400.0" prefWidth="600.0"
                   AnchorPane.topAnchor="0.0"
                   AnchorPane.bottomAnchor="0.0"
                   AnchorPane.leftAnchor="0.0"
                   AnchorPane.rightAnchor="0.0">
            <columns>
                <TableColumn fx:id="FileNameColumn" prefWidth="132.0" text="Nombre de Archivo" />
                <TableColumn fx:id="ArtistColumn" prefWidth="147.0" text="Artista" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
